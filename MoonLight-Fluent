local function formatNumber(num)
Â  Â  local suffixes = {"", "K", "M", "B", "T", "QA", "QI"}
Â  Â  local i = 1
Â  Â  while num >= 1000 and i < #suffixes do
Â  Â  Â  Â  num = num / 1000
Â  Â  Â  Â  i = i + 1
Â  Â  end
Â  Â  return string.format("%.1f%s", num, suffixes[i])
end


